public class Group
{
	int groupID;
	Vector <Visitor> visitors;
		
	public Group(int id){
		visitors = new Vector <Visitor> ();
		groupID = id;
	}//constructor
	
	public void AddToGroup(Visitor v){
		visitors.add(v);
	}//addtogroup
		
	
}//Group

