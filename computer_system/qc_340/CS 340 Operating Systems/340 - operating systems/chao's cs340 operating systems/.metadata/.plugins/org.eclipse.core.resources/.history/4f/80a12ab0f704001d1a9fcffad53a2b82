public class Project1{
	
	public static void main(String args[]){
		
		Thread Visitors[] = new Thread[7];
		
		for(int i = 0; i < 7; i++){
			Visitors[i] = new Thread(new Visitor());
			Visitors[i].start();
		}//for
		
		
	}//main
		
}//Project 1