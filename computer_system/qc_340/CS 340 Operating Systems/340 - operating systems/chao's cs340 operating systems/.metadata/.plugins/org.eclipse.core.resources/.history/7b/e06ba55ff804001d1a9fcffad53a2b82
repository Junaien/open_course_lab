public class Project1{
	
	public static void main(String args[]){
		int numVisitors = 0;
		System.out.print("Enter # of visitors: ");
		numVisitors = args[0];
		
		Thread Visitors[] = new Thread[7];
		
		for(int i = 0; i < 7; i++){
			Visitors[i] = new Thread(new Visitor(), "Visitor " + i);
			Visitors[i].start();
		}//for
		
		
	}//main
		
}//Project 1